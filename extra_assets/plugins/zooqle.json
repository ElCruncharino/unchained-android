{
  "engine_version": 1.0,
  "version": 1.0,
  "url": "https://zooqle.com",
  "name": "Zooqle",
  "description": "Parser for my zooqle.com",
  "supported_categories": {
  "all": "None"
  },
  "search": {
    "no_category": "${url}/search\\?pg={page}&q=${query}",
    "page_start": 1
  },
  "download": {
    "magnet": {
      "regex": "href=\"(magnet:\\?xt=urn:btih:[^\"]+)",
      "group": 1,
      "slug_type": "complete"
    },
    "table_direct":{
      "class": "table-torrents",
      "name_column" : 1,
      "name_regex": {
        "regex": "<a[^>]+>(.*)(?<!</a>)</a>",
        "group": 1,
        "slug_type": "complete"
      },
      "magnet_column": 2,
      "magnet_regex": {
        "regex": "href=\"(magnet:\\?xt=urn:btih:[^\"]+)",
        "group": 1,
        "slug_type": "complete"
      },
      "seeders_column": 5,
      "seeders_regex": {
        "regex": "Seeders:\\s*(\\d+)",
        "group": 1,
        "slug_type": "complete"
      },
      "leechers_column": 5,
      "leechers_regex": {
        "regex": "Leechers:\\s*(\\d+)",
        "group": 1,
        "slug_type": "complete"
      },
      "size_column": 3,
      "size_regex": {
        "regex": "([\\d.]+\\s*[A-Za-z]+)\\s*</div>",
        "group": 1,
        "slug_type": "complete"
      },
      "details_column": 1,
      "details_regex": {
        "regex": "href=\"(/[^\"]+)",
        "group": 1,
        "slug_type": "append_url"
      }
    }
  }
}